{
  "name": "careertracker",
  "version": "1.0.0",
  "description": "Multi-user job search management system with AI-powered LinkedIn integration",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cd backend && npm run dev",
    "frontend:dev": "cd frontend && npm run dev",
    "backend:start": "cd backend && npm start",
    "frontend:build": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["chrome-extension", "linkedin", "job-tracker", "ai", "career-management", "multi-user"],
  "author": "Your Name",
  "license": "MIT"
}